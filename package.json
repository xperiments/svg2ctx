{
  "name": "svg2ctx",
  "preferGlobal": true,
  "version": "0.0.7",
  "author": "xperiments aka Pedro Casaubon",
  "description": "NodeJS module for converting SVG files to Canvas context instructions",
  "main": "./src/SVGConverter.js",
  "bin": {
    "svg2ctx": "./bin/svg2ctx"
  },
  "dependencies" :
  {
    "canvas"    :"*",
    "uglify-js" :"*",
    "sprintf-js" :"*",
  	"handlebars":"*",
  	"optimist":"*",
  	"node-beautify":"*"
  },
  "bundledDependencies": [
    "fabric",
	"node-jsmin"
  ],
  "analyze": false,
  "license": "MIT",
  "engines": {
    "node": ">=0.8"
  },
  "directories": {
    "bin": "./bin",
    "src": "./src",
    "node_modules/fabric": "./node_modules/fabric",
    "node_modules/node-jsmin": "./node_modules/node-jsmin"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/xperiments/svg2ctx.git"
  }

}
